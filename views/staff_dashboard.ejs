<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>

    <link rel="stylesheet" href="../public/bootstrap.min.css">

</head>
<body>
    
   <div class="d-flex">
    <div class="d-flex flex-column flex-shrink-0 p-3 text-bg-dark vh-100" style="width: 280px;">
      <a href="/" class="d-flex align-items-center mb-3 mb-md-0 me-md-auto text-white text-decoration-none">
        <span class="fs-4">Staff</span>
      </a>
      <hr>
      <ul class="nav nav-pills flex-column mb-auto">
        <li>
          <a href="/staff_dashboard" class="nav-link text-white active">
            Dashboard
          </a>
        </li>
            <a href="/staff_result" class="nav-link text-white">
              Result
            </a>
        </li>
      </ul>
      <div>
        <a href="/staff_logout" class="text-center text-white d-block btn btn-danger text-decoration-none " >
          Logout
        </a>
      </div>
  </div>


  <div class="d-flex flex-column flex-shrink-0 p-3 vh-100 overflow-auto" style="width: calc(100vw - 280px);">
    <h3>Student Details</h3>
    <table class="table table-bordered">
      <thead>
        <th>Id</th>
        <th>Roll No</th>
        <th>Student Name</th>
        <th>Standard</th>
        <th>Division</th>
      </thead>

      <tbody>
            
        <% result.forEach(function(user){%>
            <tr>
                <td><%= user.id %></td>
                <td><%= user.rno %></td>
                <td><%= user.name %></td>
                <td><%= user.std %></td>
                <td><%= user.division %></td>
            </tr>
        <%})%>
      </tbody>
    </table><br><br>
  </div>
</body>
</html>