<h1 align="center">View Result</h1>
<table align="center" border="1">
    <thead>
      <th>Roll No</th>
      <th>Student Name</th>
      <th>Standard</th>
      <th>Division</th>
      <th>Gujarati</th>
      <th>Hindi</th>
      <th>English</th>
      <th>Maths</th>
      <th>Science</th>
      <th>Total</th>
      <th>Percentage</th>
    </thead>
  
    <tbody>
      <% result.forEach(function (data){%>
        <tr>
        <td><%= data.rno %></td>
        <td><%= data.name %></td>
        <td><%= data.std %></td>
        <td><%= data.division %></td>
        <td><%= data.gujarati %></td>
        <td><%= data.hindi %></td>
        <td><%= data.english %></td>
        <td><%= data.maths %></td>
        <td><%= data.science %></td>
        <td><%= data.total %></td>
        <td><%= data.avg %></td>
      </tr>

      <%})%>
    </tbody>
  </table>